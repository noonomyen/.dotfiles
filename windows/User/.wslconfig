[wsl2]
# --- WSL ---
safeMode=false
debugConsole=false
vmIdleTimeout=-1

# --- GUI ---
guiApplications=true

# --- SYSTEM ---
processors=12
memory=8GB
swap=4GB
#swapFile=%USERPROFILE%\AppData\Local\Temp\swap.vhdx

# --- KERNEL ---
#kernel=
#kernelCommandLine=
nestedVirtualization=true
#pageReporting=true

# --- NETWORK ---
localhostForwarding=true
dnsProxy=true
firewall=true
dnsTunneling=true
autoProxy=true

# --- Virtual Disk ---
defaultVhdSize=274877906944

# --- EXPERIMENTAL ---
[experimental]
autoMemoryReclaim=gradual
sparseVhd=true
#bestEffortDnsParsing=false
#dnsTunnelingIpAddress=10.255.255.254
#initialAutoProxyTimeout=100
#ignoredPorts=null
hostAddressLoopback=true
