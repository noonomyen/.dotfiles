[wsl2]
# kernel=
# kernelModules=
memory=16GB
processors=12
localhostForwarding=false
# kernelCommandLine=
safeMode=false
swap=8GB
# swapFile=%Temp%\swap.vhdx
## pageReporting=true
guiApplications=true
debugConsole=false
maxCrashDumpCount=10
nestedVirtualization=true
vmIdleTimeout=-1
dnsProxy=true
networkingMode=NAT
firewall=true
dnsTunneling=true
autoProxy=true
# defaultVhdSize=1099511627776

[experimental]
autoMemoryReclaim=gradual
sparseVhd=false
bestEffortDnsParsing=false
dnsTunnelingIpAddress=
initialAutoProxyTimeout=1000
# ignoredPorts=Null
hostAddressLoopback=false

